{
  "name": "hardhat-project",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "build": "npx hardhat compile",
    "clean": "npx hardhat clean",
    "build-start": "",
    "deploy:erc20": "npx hardhat deploy --network optimism_kovan --tags ERC20PresetFixedSupply --show-stack-traces",
    "deploy:calHash": "npx hardhat deploy --network optimism_kovan --tags CalHash --show-stack-traces",
    "calHash": "npx hardhat calHash --network optimism_kovan --show-stack-traces",
    "deploy:weth": "npx hardhat deploy --network optimism_kovan --tags WETH9 --show-stack-traces",
    "deploy:texFactory": "npx hardhat deploy --network optimism_kovan --tags TexFactory --show-stack-traces",
    "deploy:recorder": "npx hardhat deploy --network optimism_kovan --tags Recorder --show-stack-traces",
    "deploy:router": "npx hardhat deploy --network optimism_kovan --tags TexRouter02 --show-stack-traces",
    "factory:createPair": "npx hardhat factory:createPair --network optimism_kovan --show-stack-traces",
    "build-end": "",
    "liquidity-start": "",
    "mint": "npx hardhat mint --network optimism_kovan --show-stack-traces",
    "getReserve": "npx hardhat getReserves --network optimism_kovan --show-stack-traces",
    "addLiquidity": "npx hardhat router:addLiquidity --network optimism_kovan --show-stack-traces",
    "router:addLiquidityETH": "npx hardhat router:addLiquidityETH --network optimism_kovan --show-stack-traces",
    "router:removeLiquidity": "npx hardhat router:removeLiquidity --network optimism_kovan --show-stack-traces",
    "liquidity-end": "",
    "tex:addTxIds": "npx hardhat tex:addTxIds --network optimism_kovan --show-stack-traces",
    "tex:checkOrder": "npx hardhat tex:checkOrder --network optimism_kovan --show-stack-traces",
    "recorder:state": "npx hardhat recorder:state --network optimism_kovan --show-stack-traces",
    "tex:swap": "npx hardhat tex:swap --network optimism_kovan --show-stack-traces",
    "tex:encrypt": "npx hardhat tex:encrypt --network optimism_kovan --show-stack-traces",
    "tex:verify": "npx hardhat tex:verify --network optimism_kovan --show-stack-traces",
    "tex:decrypt": "npx hardhat tex:decrypt --network optimism_kovan --show-stack-traces",
    "router:approve": "npx hardhat router:approve --network optimism_kovan --show-stack-traces",
    "deploy:tester": "npx hardhat deploy --network optimism_kovan --tags Tester --show-stack-traces",
    "tt": "npx hardhat tt --network optimism_kovan --show-stack-traces",
    "weth:deposit": "npx hardhat weth:deposit --network optimism_kovan --show-stack-traces",
    "weth:withdraw": "npx hardhat weth:withdraw --network optimism_kovan --show-stack-traces",
    "weth:approve": "npx hardhat weth:approve --network optimism_kovan --show-stack-traces",
    "weth:transfer": "npx hardhat weth:transfer --network optimism_kovan --show-stack-traces",
    "weth:transferFrom": "npx hardhat weth:transferFrom --network optimism_kovan --show-stack-traces"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^1.0.2",
    "hardhat": "^2.11.2"
  }
}
